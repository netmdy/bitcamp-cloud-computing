<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitcamp.test.dao.UserDao">

  <select id="selectList" resultType="User" parameterType="int">
    select name from tb_user where no=#{no}
  </select>

  <select id="selectOne" resultType="User" parameterType="String">
    select * from tb_user where name=#{name}
  </select>

  <select id="selectView" resultType="User" parameterType="int">
    select * from tb_user where name=#{name}
  </select>
 
</mapper>